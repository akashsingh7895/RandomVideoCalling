<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.avssolution.videocalling_app">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RandomVideoCalling">
        <activity
            android:name="com.avssolution.videocalling_app.Activity.CallTypeActivity"
            android:exported="true" >




        </activity>
        <activity
            android:name="com.avssolution.videocalling_app.Activity.RoomActivity"
            android:exported="true"></activity>
        <activity
            android:name="com.avssolution.videocalling_app.FakeCall.FakeCallActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.avssolution.videocalling_app.Activity.PrivicyPolicyActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.avssolution.videocalling_app.Activity.ChatLishActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.avssolution.videocalling_app.Activity.GroupActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.avssolution.videocalling_app.SelectedActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.avssolution.videocalling_app.Activity.SplashActivity"
            android:exported="true"
            android:screenOrientation="portrait">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>


        </activity>
        <activity
            android:name="com.avssolution.videocalling_app.Activity.WelcomeActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.avssolution.videocalling_app.Activity.GenderActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.avssolution.videocalling_app.Activity.CallActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.avssolution.videocalling_app.Activity.ConnectingActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.avssolution.videocalling_app.Activity.LogInActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.avssolution.videocalling_app.MainActivity"
            android:exported="true" />

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="@string/admob_app_id" />

        <service
            android:name="com.avssolution.videocalling_app.Models.MyFirebaseMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>